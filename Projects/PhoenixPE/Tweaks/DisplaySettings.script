[Main]
Title=Display Settings
Author=Homes32
Description=This script allows you to customize display settings such as the default screen resolution and DPI scaling.
Version=1.0.0.0
Selected=False
Mandatory=False
Date=2025-09-12
Level=4

[Variables]

[Process]
Echo,"Configuring Display Settings..."

// Set Display Resolution
If,ExistFile,"%TargetSystem32%\PhoenixPE.Globals.au3",Begin
  TXTDelLine,"%TargetSystem32%\PhoenixPE.Globals.au3","Global $g_sDefaultScreenRes"
  TXTAddLine,"%TargetSystem32%\PhoenixPE.Globals.au3","Global $g_sDefaultScreenRes = #$q%cmb_DisplayResolution%#$q",Prepend
End

// Set DPI Scaling
Set,%LogPixels%,%cmb_DPI%
StrFormat,EndTrim,%LogPixels%,"%",%LogPixels%
StrFormat,Mult,%LogPixels%,96
StrFormat,Div,%LogPixels%,100
RegHiveLoad,Tmp_Default,%RegDefault%
// 0 = Different scale factor for each display., 1 = Same scale factor is applied to all displays.
RegWrite,HKLM,REG_DWORD,"Tmp_Default\Control Panel\Desktop","Win8DpiScaling",1
//  Scale factor that applies to all displays, regardless of each displayâ€™s pixel density (100%-500%).
RegWrite,HKLM,REG_DWORD,"Tmp_Default\Control Panel\Desktop","LogPixels",%LogPixels%
RegHiveUnload,Tmp_Default

[#SetDefaultOptions#]
// ===============================================================================================================================
// Name...........: SetDefaultOptions
// Description....: Sets script options to default values.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: Called by btn_SetDefaultOptions
// Related........: 
// ===============================================================================================================================
[SetDefaultOptions]
System,Cursor,Wait
WriteInterface,Value,%ScriptFile%,Interface,cmb_DisplayResolution,"1024 x 768"
WriteInterface,Value,%ScriptFile%,Interface,cmb_DPI,"100%"
System,Cursor,Normal

