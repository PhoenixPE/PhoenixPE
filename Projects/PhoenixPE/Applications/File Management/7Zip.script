[Main]
Title=7-Zip
Author=Homes32
Level=5
Version=1.2.8.0
Description=(v25.01) 7-Zip is a free and open-source file archiver capable of reading and writing many archive formats.
Date=2025-09-17
Selected=True
Mandatory=False

[Variables]
%ProgramFolder%="7-Zip"
%ProgramExe%="7zFM.exe"
%ProgramVersion%=2501
%DownloadURL86%=https://www.7-zip.org/a/7z%ProgramVersion%.exe
%DownloadURL64%=https://www.7-zip.org/a/7z%ProgramVersion%-x64.exe

[Process]
Echo,"Processing %ScriptTitle%..."

If,%cb_RunFromRam%,Equal,True,RunFromRam

StrFormat,SPLIT,%cmb_Language%,"[",2,%7zLang%
StrFormat,RTRIM,%7zLang%,1,%7zLang%

///////////////////////////////////////////////////////////////////////////////////
// Extract
If,Not,ExistFile,"%ProgramsCache%\%ProgramFolder%\Setup-%SourceArch%.exe",Run,%ScriptFile%,DownloadProgram
Run,%ScriptFile%,ExtractProgram,"%TargetPrograms%\%ProgramFolder%"

///////////////////////////////////////////////////////////////////////////////////
// Settings

Echo,"Configuring %ScriptTitle%..."
RegHiveLoad,Tmp_Software,%RegSoftware%
RegHiveLoad,Tmp_Default,%RegDefault%

RegWrite,HKLM,0x1,"Tmp_Default\Software\7-Zip","Lang","%7zLang%"

RegWrite,HKLM,0x2,"Tmp_Default\Software\7-Zip","Path","%PEPrograms%\%ProgramFolder%\7-Zip"

Run,%ScriptFile%,RegisterFileAssociations

If,%cb_AddShellContextMenu%,Equal,True,Begin
  
  // Register Shell Extension
  RegWrite,HKLM,0x2,"Tmp_Software\7-Zip","Path","%PEPrograms%\%ProgramFolder%"
  RegWrite,HKLM,0x2,"Tmp_Software\Microsoft\Windows\CurrentVersion\App Paths\7zFM.exe","","%PEPrograms%\%ProgramFolder%\%ProgramExe%"
  RegWrite,HKLM,0x2,"Tmp_Software\Microsoft\Windows\CurrentVersion\App Paths\7zFM.exe","Path","%PEPrograms%\%ProgramFolder%"
  
  RegWrite,HKLM,0x1,"Tmp_Software\Classes\*\shellex\ContextMenuHandlers\7-Zip","","{23170F69-40C1-278A-1000-000100020000}"
  RegWrite,HKLM,0x1,"Tmp_Software\Classes\CLSID\{23170F69-40C1-278A-1000-000100020000}","","7-Zip Shell Extension"
  RegWrite,HKLM,0x2,"Tmp_Software\Classes\CLSID\{23170F69-40C1-278A-1000-000100020000}\InprocServer32","","%PEPrograms%\%ProgramFolder%\7-zip.dll"
  RegWrite,HKLM,0x1,"Tmp_Software\Classes\CLSID\{23170F69-40C1-278A-1000-000100020000}\InprocServer32","ThreadingModel","Apartment"
  RegWrite,HKLM,0x1,"Tmp_Software\Classes\Directory\shellex\ContextMenuHandlers\7-Zip","","{23170F69-40C1-278A-1000-000100020000}"
  RegWrite,HKLM,0x1,"Tmp_Software\Classes\Directory\shellex\DragDropHandlers\7-Zip","","{23170F69-40C1-278A-1000-000100020000}"
  RegWrite,HKLM,0x1,"Tmp_Software\Classes\Drive\shellex\DragDropHandlers\7-Zip","","{23170F69-40C1-278A-1000-000100020000}"
  RegWrite,HKLM,0x1,"Tmp_Software\Microsoft\Windows\CurrentVersion\Shell Extensions\Approved","{23170F69-40C1-278A-1000-000100020000}","7-Zip Shell Extension"

  // Cascade Menu
  If,%cb_CascadedContextMenu%,Equal,True,RegWrite,HKLM,0x4,"Tmp_Default\Software\7-Zip\Options","CascadedMenu",1
  Else,RegWrite,HKLM,0x4,"Tmp_Default\Software\7-Zip\Options","CascadedMenu",0

  // Context Menu Items
  // Context menu items are stored as a DWORD. Items can be determined by bitshifting the value.
  // Extract = 1 << 0; ExtractHere = 1 << 1; ExtractTo = 1 << 2; Test = 1 << 4; Open = 1 << 5; OpenAs = 1 << 6; Compress = 1 << 8;
  // CompressTo7z = 1 << 9; CompressEmail = 1 << 10; CompressTo7zEmail = 1 << 11; CompressToZip = 1 << 12; CompressToZipEmail = 1 << 13; CRC = 1 << 31;
  Set,%ContextMenuItems%,0
  If,%cb_ContextMenuOpenArchive%,Equal,True,Math,Add,%ContextMenuItems%,%ContextMenuItems%,32
  If,%cb_ContextMenuAddToArchive%,Equal,True,Math,Add,%ContextMenuItems%,%ContextMenuItems%,256
  If,%cb_ContextMenuAddTo7z%,Equal,True,Math,Add,%ContextMenuItems%,%ContextMenuItems%,512
  If,%cb_ContextMenuAddToZip%,Equal,True,Math,Add,%ContextMenuItems%,%ContextMenuItems%,4096
  If,%cb_ContextMenuExtractFiles%,Equal,True,Math,Add,%ContextMenuItems%,%ContextMenuItems%,1
  If,%cb_ContextMenuExtractHere%,Equal,True,Math,Add,%ContextMenuItems%,%ContextMenuItems%,2
  If,%cb_ContextMenuExtractToFolder%,Equal,True,Math,Add,%ContextMenuItems%,%ContextMenuItems%,4
  If,%cb_ContextMenuCRC%,Equal,True,Math,Add,%ContextMenuItems%,%ContextMenuItems%,2147483648
  RegWrite,HKLM,0x4,"Tmp_Default\Software\7-Zip\Options","ContextMenu",%ContextMenuItems%

End

RegHiveUnLoad,Tmp_Software
RegHiveUnLoad,Tmp_Default

///////////////////////////////////////////////////////////////////////////////////
// Shortcuts
If,%cb_DesktopShc%,Equal,True,AddShortcut,Desktop,"",%txt_ShortcutName%,"%PEPrograms%\%ProgramFolder%\%ProgramExe%","","%PEPrograms%\%ProgramFolder%"
If,%cb_StartMenuShc%,Equal,True,AddShortcut,StartMenu,%txt_StartMenuFolder%,%txt_ShortcutName%,"%PEPrograms%\%ProgramFolder%\%ProgramExe%","","%PEPrograms%\%ProgramFolder%"
If,%cb_PinToTaskbar%,Equal,True,PinShortcut,Taskbar,Auto,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"
If,%cb_PinToStartMenu%,Equal,True,PinShortcut,StartMenu,Auto,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"

