[Main]
Title=HxD Hex Editor
Description=HxD analysiert und manipuliert Binärdateien und öffnet sogar Festplatten und einzelne Partitionen, um etwa Bootsektoren zu manipulieren.
Author=ct
Level=5
Selected=True
Mandatory=False
Version=1.0.0.0
Date=2022-11-07

[Variables]
%ProgramTitle%=HxD Hex Editor
%ProgramExe%=HxD32.exe
%ProgramExex64%=HxD64.exe
%ProgramFolder%=HxD
%SetupFile%=HxDPortableSetup.exe
%DownFile%=HxD.zip
%DownloadURL%=https://mh-nexus.de/downloads/HxDPortableSetup.zip

[Process]
Echo,"Processing %ScriptTitle%..."
RunFromRam
Run,%ScriptFile%,SetProgramArch
If,%cb_AlwaysDownload%,Equal,True,DirDeleteEx,"%ProgramsCache%\%ProgramFolder%"
If,Not,ExistFile,"%ProgramsCache%\%ProgramFolder%\%DownFile%",Run,%ScriptFile%,DownloadProgram
Run,%ScriptFile%,Extract
If,%SourceArch%,Equal,x64,FileDelete,"%TargetPrograms%\%ProgramFolder%\HxD32.exe"
If,%SourceArch%,Equal,x84,FileDelete,"%TargetPrograms%\%ProgramFolder%\HxD64.exe"

///////////////////////////////////////////////////////////////////////////////////
// Shortcuts

If,%cb_DesktopShc%,Equal,True,AddShortcut,Desktop,,%txt_ShortcutName%,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"
If,%cb_StartMenuShc%,Equal,True,AddShortcut,StartMenu,%txt_StartMenuFolder%,%txt_ShortcutName%,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"
If,%cb_PinToTaskbar%,Equal,True,PinShortcut,Taskbar,Auto,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"
If,%cb_PinToStartMenu%,Equal,True,PinShortcut,StartMenu,Auto,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"

[Extract]
7z,"x -r #$q%ProgramsCache%\%ProgramFolder%\%DownFile%#$q -y -o#$q%ProgramsCache%\%ProgramFolder%#$q"
ShellExecute,Hide,"%Tools%\x86\innoextract.exe","-e #$q%ProgramsCache%\%ProgramFolder%\%SetupFile%#$q#$s-d#$q%TargetPrograms%\%ProgramFolder%#$q"
FileDelete,"%TargetPrograms%\%ProgramFolder%\app\HxD.exe"
FileCopy,"%TargetPrograms%\%ProgramFolder%\app\*.*","%TargetPrograms%\%ProgramFolder%"
If,ExistDir,"%TargetPrograms%\%ProgramFolder%\app",DirDelete,"%TargetPrograms%\%ProgramFolder%\app"

[DownloadProgram]
Echo,"Downloading %ScriptTitle%..."
WebGet,%DownloadURL%,"%ProgramsCache%\%ProgramFolder%\%DownFile%",NOERR
If,Not,#r,Equal,200,Halt,"Download failed: The code returned was [#r]."

[SetProgramArch]
If,%SourceArch%,Equal,x64,Begin
  Set,%ProgramExe%,%ProgramExex64%
End  

[LaunchProgram]
Echo,"Launching %ScriptTitle%..."
Run,%ScriptFile%,SetProgramArch
If,Not,ExistFile,"%ProgramsCache%\%ProgramFolder%\%DownFile%",Run,%ScriptFile%,DownloadProgram
If,Not,ExistFile,"%%TargetPrograms%\%ProgramFolder%\%ProgramExe%",Run,%ScriptFile%,Extract
ShellExecuteEx,Open,"%TargetPrograms%\%ProgramFolder%\%ProgramExe%"

[ClearDownloadCache]
DirDeleteEx,"%ProgramsCache%\%ProgramFolder%"

[SetDefaultOptions]
System,Cursor,Wait
WriteInterface,Value,%ScriptFile%,Interface,cb_RunFromRam,False
WriteInterface,Value,%ScriptFile%,Interface,cb_DesktopShc,False
WriteInterface,Value,%ScriptFile%,Interface,cb_StartMenuShc,True
WriteInterface,Value,%ScriptFile%,Interface,cb_PinToTaskbar,False
WriteInterface,Value,%ScriptFile%,Interface,cb_PinToStartMenu,False
WriteInterface,Value,%ScriptFile%,Interface,txt_StartMenuFolder,"System Tools"
WriteInterface,Value,%ScriptFile%,Interface,txt_ShortcutName,"HxD Editor"
WriteInterface,Value,%ScriptFile%,Interface,cb_AlwaysDownload,False
System,Cursor,Normal

[ToggleAdvancedOptions]
Message,"There are no advanced options available for this script."

[ShowScriptInfo]
Message,"Mit HxD lassen sich Binärdateien analysieren und manipulieren. HxD öffnet auch Festplatten und einzelne Partitionen, sodass man etwa Bootsektoren manipulieren kann."


