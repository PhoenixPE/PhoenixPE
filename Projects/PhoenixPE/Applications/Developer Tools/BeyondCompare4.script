RegWrite,HKLM,0x1,"Tmp_Software\WOW6432Node\Scooter Software\Beyond Compare 4","Version","%ProgramVer%"

RegWrite,HKLM,0x1,"Tmp_Default\Software\Scooter Software\Beyond Compare","ExePath","%PEPrograms%\%ProgramFolder%\%ProgramExe%"
RegWrite,HKLM,0x1,"Tmp_Default\Software\Scooter Software\Beyond Compare","Version","%ProgramVer%"
RegWrite,HKLM,0x1,"Tmp_Default\Software\Scooter Software\Beyond Compare 4","ExePath","%PEPrograms%\%ProgramFolder%\%ProgramExe%"
RegWrite,HKLM,0x1,"Tmp_Default\Software\Scooter Software\Beyond Compare 4","Version","%ProgramVer%"

