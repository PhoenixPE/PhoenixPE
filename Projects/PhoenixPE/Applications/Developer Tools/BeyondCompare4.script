// Misc
RegWrite,HKLM,0x1,"Tmp_Software\Scooter Software\Beyond Compare","ExePath","%PEPrograms%\%ProgramFolder%\%ProgramExe%"
RegWrite,HKLM,0x1,"Tmp_Software\Scooter Software\Beyond Compare","Version","%ProgramVer%"
