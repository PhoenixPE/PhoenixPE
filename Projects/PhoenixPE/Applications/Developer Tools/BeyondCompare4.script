// AppPath
RegWrite,HKLM,0x1,"Tmp_Software\Microsoft\Windows\CurrentVersion\App Paths\BCompare.exe","","%PEPrograms%\%ProgramFolder%\%ProgramExe%"
RegWrite,HKLM,0x4,"Tmp_Software\Microsoft\Windows\CurrentVersion\App Paths\BCompare.exe","UseURL",1

// File Extensions
RegWrite,HKLM,0x1,"Tmp_Software\Classes\.bcpkg","","BeyondCompare.SettingsPackage"
