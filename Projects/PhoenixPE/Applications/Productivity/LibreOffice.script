[Main]
Title=LibreOffice
Author=Homes32
Level=5
Version=1.0.5.0
Description=(v7.6.4) LibreOffice is a free and powerful office suite.
Date=2024-01-13
Selected=False
Mandatory=False

[Variables]
%ProgramFolder%="LibreOffice"
%ProgramVersion%=7.6.4
%ProgramExe%="program\soffice.exe"
%DownloadURL-Std%=https://download.documentfoundation.org/libreoffice/portable/%ProgramVersion%/LibreOfficePortable_%ProgramVersion%_MultilingualStandard.paf.exe
%DownloadURL-AllLang%=https://download.documentfoundation.org/libreoffice/portable/%ProgramVersion%/LibreOfficePortable_%ProgramVersion%_MultilingualStandard.paf.exe
%SetupFile%="LibreOfficePortable-Std.paf.exe"
%registrymodifications.xcu%="%TargetAppData%\%ProgramFolder%\user\registrymodifications.xcu"
%StdLangList%=Arabic [ar]|Chinese (Simplified) [zh-CN]|Chinese (Traditional) [zh-TW]|English (United Kingdom) [en-GB]|English [en-US]|French [fr]|German [de]|Italian [it]|Japanese [ja]|Korean [ko]|Portuguese (Brazil) [pt-BR]|Portuguese [pt]|Russian [ru]|Spanish [es]

[Process]
Echo,"Processing %ScriptTitle%..."

If,%cb_RunFromRam%,Equal,True,RunFromRam
Run,%ScriptFile%,SetProgramRelease

If,Not,ExistFile,"%ProgramsCache%\%ProgramFolder%\%SetupFile%",Run,%ScriptFile%,DownloadProgram

///////////////////////////////////////////////////////////////////////////////////
// Extract

DirDeleteEx,"%TargetPrograms%\%ProgramFolder%"
Run,%ScriptFile%,ExtractProgram,"%ProjectTemp%\%ProgramFolder%"
FileCopy,"%ProjectTemp%\%ProgramFolder%\App\libreoffice\*.*","%TargetPrograms%\%ProgramFolder%"

// Default Settings
FileCopy,"%ProjectTemp%\%ProgramFolder%\App\DefaultData\settings\*.*","%TargetAppData%\%ProgramFolder%"

// Dependencies
RequireFileEx,AppendList,\Windows\System32\DiagnosticDataSettings.dll
RequireFileEx,AppendList,\Windows\System32\fontsub.dll
RequireFileEx,AppendList,\Windows\System32\opencl.dll
RequireFileEx,AppendList,\Windows\System32\umpdc.dll
RequireFileEx,AppendList,\Windows\System32\wer.dll

// WoW64
If,ExistFile,"%TargetSystem32%\wow64.dll",Begin
  RequireFileEx,AppendList,\Windows\SysWOW64\DiagnosticDataSettings.dll
  RequireFileEx,AppendList,\Windows\SysWOW64\fontsub.dll
  RequireFileEx,AppendList,\Windows\SysWOW64\opencl.dll
  RequireFileEx,AppendList,\Windows\SysWOW64\umpdc.dll
  RequireFileEx,AppendList,\Windows\SysWOW64\wer.dll
End

// Extract
RequireFileEx,ExtractList

///////////////////////////////////////////////////////////////////////////////////
// Settings

// registrymodifications.xcu uses a convoluted XML schema that makes using XPATH extremely difficult, so we are using TXTReplace to append to the end of the file in those instances where we know the entry doesn't already exist

Echo,"Configuring LibreOffice..."

// Configure our settings folder. Note: Use UNIX style path!
IniWrite,"%TargetPrograms%\%ProgramFolder%\program\bootstrap.ini","Bootstrap","UserInstallation","$SYSUSERCONFIG/%ProgramFolder%"

// Language
StrFormat,SPLIT,%cmb_Language%,"[",2,%lng%
StrFormat,RTRIM,%lng%,1,%lng%

IniWrite,"%TargetPrograms%\%ProgramFolder%\program\soffice.ini","Bootstrap","STARTLANG",%lng%
XMLUpdate,%registrymodifications.xcu%,"/oor:items/item/prop[@oor:name='UILocale']/value",%lng%
XMLUpdate,%registrymodifications.xcu%,"/oor:items/item/prop[@oor:name='ooLocale']/value",%lng%

// Disable file ext check, TipOfTheDay, etc.
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.Common/Help#$q><prop oor:name=#$qBuiltInHelpNotInstalledPopUp#$q oor:op=#$qfuse#$q><value>false</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.Common/Misc#$q><prop oor:name=#$qFirstRun#$q oor:op=#$qfuse#$q><value>false</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.Common/Misc#$q><prop oor:name=#$qPerformFileExtCheck#$q oor:op=#$qfuse#$q><value>false</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.Common/Misc#$q><prop oor:name=#$qShowTipOfTheDay#$q oor:op=#$qfuse#$q><value>false</value></prop></item>#$x</oor:items>"

// Default Folders
TXTReplace,%registrymodifications.xcu%,"<value>XXX</value>","<value>$TEMP</value>"
TXTReplace,%registrymodifications.xcu%,"<value>DEFAULT--WORK--PATH</value>","<value>$SYSUSERHOME</value>"

// Disable Update Check
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.Jobs/Jobs/org.openoffice.Office.Jobs:Job['UpdateCheck']/Arguments#$q><prop oor:name=#$qAutoCheckEnabled#$q oor:op=#$qfuse#$q oor:type=#$qxs:boolean#$q><value>false</value></prop></item>#$x</oor:items>"

// Set MS Office as default document types
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Setup/Office/Factories/org.openoffice.Setup:Factory['com.sun.star.text.TextDocument']#$q><prop oor:name=#$qooSetupFactoryDefaultFilter#$q oor:op=#$qfuse#$q><value>MS Word 2007 XML</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Setup/Office/Factories/org.openoffice.Setup:Factory['com.sun.star.sheet.SpreadsheetDocument']#$q><prop oor:name=#$qooSetupFactoryDefaultFilter#$q oor:op=#$qfuse#$q><value>Calc MS Excel 2007 XML</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Setup/Office/Factories/org.openoffice.Setup:Factory['com.sun.star.presentation.PresentationDocument']#$q><prop oor:name=#$qooSetupFactoryDefaultFilter#$q oor:op=#$qfuse#$q><value>Impress MS PowerPoint 2007 XML</value></prop></item>#$x</oor:items>"

// Toolbar Mode
If,%cmb_UIMode%,Equal,"Standard Toolbar",Set,%ToolbarMode%,"Default"
Else,If,%cmb_UIMode%,Equal,"Tabbed",Set,%ToolbarMode%,"notebookbar.ui"
Else,If,%cmb_UIMode%,Equal,"Single Toolbar",Set,%ToolbarMode%,"Single"
Else,If,%cmb_UIMode%,Equal,"Sidebar",Set,%ToolbarMode%,"Sidebar"
Else,If,%cmb_UIMode%,Equal,"Tabbed Compact",Set,%ToolbarMode%,"notebookbar_compact.ui"
Else,If,%cmb_UIMode%,Equal,"Groupedbar Compact",Set,%ToolbarMode%,"notebookbar_groupedbar_compact.ui"
Else,If,%cmb_UIMode%,Equal,"Contextual Single",Set,%ToolbarMode%,"notebookbar_single.ui"
Else,Halt,"INTERNAL LOGIC ERROR: Invalid UI Mode [%cmb_UIMode%] Selected."

TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.UI.ToolbarMode#$q><prop oor:name=#$qActiveCalc#$q oor:op=#$qfuse#$q><value>%ToolbarMode%</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.UI.ToolbarMode/Applications/org.openoffice.Office.UI.ToolbarMode:Application['Calc']#$q><prop oor:name=#$qActive#$q oor:op=#$qfuse#$q><value>%ToolbarMode%</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.UI.ToolbarMode#$q><prop oor:name=#$qActiveDraw#$q oor:op=#$qfuse#$q><value>%ToolbarMode%</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.UI.ToolbarMode/Applications/org.openoffice.Office.UI.ToolbarMode:Application['Draw']#$q><prop oor:name=#$qActive#$q oor:op=#$qfuse#$q><value>%ToolbarMode%</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.UI.ToolbarMode#$q><prop oor:name=#$qActiveImpress#$q oor:op=#$qfuse#$q><value>%ToolbarMode%</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.UI.ToolbarMode/Applications/org.openoffice.Office.UI.ToolbarMode:Application['Impress']#$q><prop oor:name=#$qActive#$q oor:op=#$qfuse#$q><value>%ToolbarMode%</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.UI.ToolbarMode#$q><prop oor:name=#$qActiveWriter#$q oor:op=#$qfuse#$q><value>%ToolbarMode%</value></prop></item>#$x</oor:items>"
TXTReplace,%registrymodifications.xcu%,"</oor:items>","<item oor:path=#$q/org.openoffice.Office.UI.ToolbarMode/Applications/org.openoffice.Office.UI.ToolbarMode:Application['Writer']#$q><prop oor:name=#$qActive#$q oor:op=#$qfuse#$q><value>%ToolbarMode%</value></prop></item>#$x</oor:items>"

// File Associations
If,%cb_AssociateFiles%,Equal,True,Begin
RegHiveLoad,Tmp_Software,%RegSoftware%
  ExtractFile,%ScriptFile%,Reg,LibreOffice-Classes.reg,%ProjectTemp%

  // .reg files require escaping `\`
  StrFormat,REPLACE,%PEPrograms%,"\","\\",%esc_PEPrograms%
  StrFormat,REPLACE,%ProgramFolder%,"\","\\",%esc_ProgramFolder%
  TXTReplace,"%ProjectTemp%\LibreOffice-Classes.reg","C:\\Program Files\\LibreOffice","%esc_PEPrograms%\\%esc_ProgramFolder%"

  RegImport,"%ProjectTemp%\LibreOffice-Classes.reg"
  RegHiveUnLoad,Tmp_Software
End

// Exclude from Windows Error Reporting, LibreOffice has it's own crash handler.
RegHiveLoad,Tmp_Default,%RegDefault%
RegWrite,HKLM,0x4,"Tmp_Default\Software\Microsoft\Windows\Windows Error Reporting\ExcludedApplications","SOFFICE.EXE",1
RegHiveUnLoad,Tmp_Default

///////////////////////////////////////////////////////////////////////////////////
// Shortcuts
If,%cb_DesktopShc%,Equal,True,AddShortcut,Desktop,,%txt_ShortcutName%,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"
If,%cb_StartMenuShc%,Equal,True,AddShortcut,StartMenu,%txt_StartMenuFolder%,%txt_ShortcutName%,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"
If,%cb_PinToTaskbar%,Equal,True,PinShortcut,Taskbar,Auto,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"
If,%cb_PinToStartMenu%,Equal,True,PinShortcut,StartMenu,Auto,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"

[#ExtractProgram#]
// ===============================================================================================================================
// Name...........: ExtractProgram
// Description....: Extract program files from script/setup files.
// Syntax.........: Run,%ScriptFile%,ExtractProgram,<DestDir>
// Parameters.....: #1 DestDir - The directory where the files will be extracted.
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: 
// ===============================================================================================================================
[ExtractProgram]
Echo,"Extracting program files...#$x#$xThis can take awhile, please be patient."
7z,"x -r -y #$q%ProgramsCache%\%ProgramFolder%\%SetupFile%#$q * -xr!$PLUGINSDIR -o#$q#1#$q"

[#DownloadProgram#]
// ===============================================================================================================================
// Name...........: DownloadProgram
// Description....: Download the program files from the author's website.
// Syntax.........: Run,%ScriptFile%,DownloadProgram
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: 
// ===============================================================================================================================
[DownloadProgram]
Echo,"Downloading %ScriptTitle%..."
Run,%ScriptFile%,SetProgramRelease
WebGet,%DownloadURL%,"%ProgramsCache%\%ProgramFolder%\%SetupFile%",NOERR
If,Not,#r,Equal,200,Halt,"Failed to download [%SetupFile%]. The code returned was [#r]."

[#SetLangRelease#]
// ===============================================================================================================================
// Name...........: SetLangRelease
// Description....: Configure the script for the selected program release (Std/AllLang) based on the Language selection.
// Syntax.........: Run,%ScriptFile%,SetLangRelease
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: At present, this function is only attached to cmb_Language, in order to allow users to override the
//                  auto language selection via the Advanced Options button, and directly set rbt_Release-Std or rbtn_Release-AllLang
// Related........: cmb_Language, rbtn_Release-AllLang, rbtn_Release-Std
// ===============================================================================================================================
[SetLangRelease]
List,Pos,%StdLangList%,%cmb_Language%,%IsLangStd%

If,%IsLangStd%,Equal,0,Begin
  // Needs All Language Package
  WriteInterface,Value,%ScriptFile%,Interface,rbtn_Release-Std,False
  WriteInterface,Value,%ScriptFile%,Interface,rbtn_Release-AllLang,True
End  
Else,Begin
  // Std Package
  WriteInterface,Value,%ScriptFile%,Interface,rbtn_Release-Std,True
  WriteInterface,Value,%ScriptFile%,Interface,rbtn_Release-AllLang,False
End

[#SetProgramRelease#]
// ===============================================================================================================================
// Name...........: SetProgramRelease
// Description....: Configure the script for the selected program release (Std/AllLang).
// Syntax.........: Run,%ScriptFile%,SetProgramRelease
// Parameters.....: 
// Return values..: Sets the following local variables:
//                  %DownloadURL%    - The Std or AllLang download URL for %SetupFile%.
//                  %SetupFile%      - The Std or AllLang setup file.
// Author.........: Homes32
// Remarks........:
// Related........: 
// ===============================================================================================================================
[SetProgramRelease]
If,%rbtn_Release-Std%,Equal,True,Begin
  Set,%DownloadURL%,%DownloadURL-Std%
  Set,%SetupFile%,"LibreOfficePortable-Std.paf.exe"
End  
Else,If,%rbtn_Release-AllLang%,Equal,True,Begin
  Set,%DownloadURL%,%DownloadURL-AllLang%
  Set,%SetupFile%,"LibreOfficePortable-AllLang.paf.exe"
End
Else,Halt,"INTERNAL LOGIC ERROR: Invalid Release Selected."

[#LaunchProgram#]
// ===============================================================================================================================
// Name...........: LaunchProgram
// Description....: Unpack and launch the program.
// Syntax.........: Run,%ScriptFile%,LaunchProgram
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_LaunchProgram
// ===============================================================================================================================
[LaunchProgram]
Echo,"Launching %ScriptTitle%..."
Run,%ScriptFile%,SetProgramRelease
If,Not,ExistFile,"%ProgramsCache%\%ProgramFolder%\%SetupFile%",Run,%ScriptFile%,DownloadProgram
ShellExecuteEx,Open,"%ProgramsCache%\%ProgramFolder%\%SetupFile%"

[#ClearDownloadCache#]
// ===============================================================================================================================
// Name...........: ClearDownloadCache
// Description....: Remove all downloaded setup and program files.
// Syntax.........: Run,%ScriptFile%,ClearDownloadCache
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_PurgeCache
// ===============================================================================================================================
[ClearDownloadCache]
DirDeleteEx,"%ProgramsCache%\%ProgramFolder%"

[#SetDefaultOptions#]
// ===============================================================================================================================
// Name...........: SetDefaultOptions
// Description....: Sets script options to default values.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: 
// Related........: btn_SetDefaultOptions
// ===============================================================================================================================
[SetDefaultOptions]
System,Cursor,Wait
WriteInterface,Value,%ScriptFile%,Interface,cb_RunFromRam,False
WriteInterface,Value,%ScriptFile%,Interface,cb_DesktopShc,False
WriteInterface,Value,%ScriptFile%,Interface,cb_StartMenuShc,True
WriteInterface,Value,%ScriptFile%,Interface,cb_PinToTaskbar,False
WriteInterface,Value,%ScriptFile%,Interface,cb_PinToStartMenu,False
WriteInterface,Value,%ScriptFile%,Interface,txt_StartMenuFolder,"Productivity"
WriteInterface,Value,%ScriptFile%,Interface,txt_ShortcutName,"LibreOffice"
WriteInterface,Value,%ScriptFile%,Interface,rbtn_Release-Std,True
WriteInterface,Value,%ScriptFile%,Interface,rbtn_Release-AllLang,False
WriteInterface,Value,%ScriptFile%,Interface,cmb_Language,"English [en-US]"
WriteInterface,Value,%ScriptFile%,Interface,cmb_UIMode,"Standard Toolbar"
WriteInterface,Value,%ScriptFile%,Interface,cb_AssociateFiles,True
System,Cursor,Normal

[#ToggleAdvancedOptions#]
// ===============================================================================================================================
// Name...........: ToggleAdvancedOptions
// Description....: Show/Hide advanced options.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_AdvancedOptions
// ===============================================================================================================================
[ToggleAdvancedOptions]
System,SetLocal
ReadInterface,Visible,%ScriptFile%,Interface,bvl_DownloadRelease,%state%
If,%state%,Equal,False,Begin
  If,Question,"WARNING!#$x#$xAdvanced options are intended for expert users only!#$x#$xModifying advanced options without knowing what you are doing can cause permanent damage to your project and may result in unexpected behavior and/or build failures.#$x#$xAre you sure you want to continue?",Begin
	Set,%Toggle%,True
    WriteInterface,Resource,%ScriptFile%,Interface,btn_AdvancedOptions,Advanced_Exit_16.png
	WriteInterface,Tooltip,%ScriptFile%,Interface,btn_AdvancedOptions,"Hide Advanced Options"
  End
  Else,Set,%Toggle%,False
End
Else,Begin
  Set,%Toggle%,False
  WriteInterface,Resource,%ScriptFile%,Interface,btn_AdvancedOptions,Advanced_16.png
  WriteInterface,Tooltip,%ScriptFile%,Interface,btn_AdvancedOptions,"Show Advanced Options"
End

WriteInterface,Visible,%ScriptFile%,Interface,lbl_Release,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,rbtn_Release-Std,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,rbtn_Release-AllLang,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,bvl_DownloadRelease,%Toggle%
System,EndLocal

