[Main]
Title=Macrium Reflect
Description=Erstellt Disk-Images.
Author=ct
Level=5
Selected=True
Mandatory=False
Version=1.0.0.0
Date=2022-11-07

[Variables]
%ProgramTitle%=Macrium Reflect
%ProgramExe%=Reflect.exe
%ProgramFolder%=Macrium Reflect
%ProgramFolderx64%=Macrium Reflect_x64
%SetupFile%=reflect_setup_free_x86.exe
%SetupFilex64%=reflect_setup_free_x64.exe
%DownloadURL%=https://download.macrium.com/reflect/v8/v8.0.7097/reflect_setup_free_x86.exe
%DownloadURLx64%=https://download.macrium.com/reflect/v8/v8.0.7097/reflect_setup_free_x64.exe


[Process]
Echo,"Processing %ScriptTitle%..."
If,%cb_RunFromRam%,Equal,True,RunFromRam
Run,%ScriptFile%,SetProgramArch
If,Not,ExistFile,"%ProgramsCache%\%ProgramFolder%\%SetupFile%",Run,%ScriptFile%,DownloadProgram
Run,%ScriptFile%,ExtractProgram

///////////////////////////////////////////////////////////////////////////////////
// Shortcuts

If,%cb_DesktopShc%,Equal,True,AddShortcut,Desktop,,%txt_ShortcutName%,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"
If,%cb_StartMenuShc%,Equal,True,AddShortcut,StartMenu,%txt_StartMenuFolder%,%txt_ShortcutName%,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"
If,%cb_PinToTaskbar%,Equal,True,PinShortcut,Taskbar,Auto,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"
If,%cb_PinToStartMenu%,Equal,True,PinShortcut,StartMenu,Auto,"%PEPrograms%\%ProgramFolder%\%ProgramExe%"

[ExtractProgram]
If,%cb_AlwaysDownload%,Equal,True,DirDeleteEx,Run,%ScriptFile%,DownloadProgram
7z,"x -r #$q%ProgramsCache%\%ProgramFolder%\%SetupFile%#$q -y -o#$q%ProjectTemp%\%ProgramFolder%#$q"
7z,"x -r #$q%ProjectTemp%\%ProgramFolder%\LibFiles.Cab#$q -y -o#$q%ProjectTemp%\%ProgramFolder%\Libfiles#$q"
7z,"x -r #$q%ProjectTemp%\%ProgramFolder%\Reflect.Cab#$q -y -o#$q%ProjectTemp%\%ProgramFolder%\Reflect#$q"
FileCopy,"%ProjectTemp%\%ProgramFolder%\Reflect\*.*","%TargetPrograms%\%ProgramFolder%"
FileCopy,"%ProjectTemp%\%ProgramFolder%\Libfiles\*.*","%TargetPrograms%\%ProgramFolder%"
// clean up
DirDelete,%ProjectTemp%\%ProgramFolder%\Libfiles

[DownloadProgram]
Echo,"Downloading %ScriptTitle%..."
WebGet,%DownloadURL%,"%ProgramsCache%\%ProgramFolder%\%SetupFile%",NOERR
If,Not,#r,Equal,200,Halt,"Download failed: The code returned was [#r]."

[SetProgramArch]
If,%SourceArch%,Equal,x64,Begin
  Set,%SetupFile%,%SetupFilex64%
  Set,%ProgramFolder%,%ProgramFolderx64%
  Set,%DownloadURL%,%DownloadURLx64%
End  

[LaunchProgram]
Run,%ScriptFile%,SetProgramArch
Echo,"Launching %ScriptTitle%..."
If,Not,ExistFile,"%TargetPrograms%\%ProgramFolder%\%ProgramExe%",Run,%ScriptFile%,ExtractProgram
ShellExecuteEx,Open,"%TargetPrograms%\%ProgramFolder%\%ProgramExe%"

[ClearDownloadCache]
DirDeleteEx,"%ProgramsCache%\%ProgramFolder%"

[SetDefaultOptions]
System,Cursor,Wait
WriteInterface,Value,%ScriptFile%,Interface,cb_RunFromRam,False
WriteInterface,Value,%ScriptFile%,Interface,cb_DesktopShc,False
WriteInterface,Value,%ScriptFile%,Interface,cb_StartMenuShc,True
WriteInterface,Value,%ScriptFile%,Interface,cb_PinToTaskbar,False
WriteInterface,Value,%ScriptFile%,Interface,cb_PinToStartMenu,False
WriteInterface,Value,%ScriptFile%,Interface,txt_StartMenuFolder,"Backup & Imaging"
WriteInterface,Value,%ScriptFile%,Interface,txt_ShortcutName,"Macrium Reflect"
WriteInterface,Value,%ScriptFile%,Interface,cb_AlwaysDownload,False
System,Cursor,Normal

[ToggleAdvancedOptions]
Message,"There are no advanced options available for this script."

[ShowScriptInfo]
Message,"Macrium Reflect erstellt Disk-Images."

