[Main]
Title=Zulu JRE 21
Description=(v21.40.17) Azul Zulu OpenJDK Java 21 (LTS) Runtime Environment
Author=Homes32
Level=4
Selected=False
Mandatory=False
Version=1.0.1.0
Date=2025-03-03

[Variables]
%ProgramFolder%="Zulu21"
%ZuluVersionx64%="21.40.17"
%JREVersion%="21.0.6"
%DownloadURLx64%=https://cdn.azul.com/zulu/bin/zulu%ZuluVersionx64%-ca-jre%JREVersion%-win_x64.zip
%SetupFile%="zulu-17-%SourceArch%.zip"
%ZipFolder64%="zulu%ZuluVersionx64%-ca-jre%JREVersion%-win_x64"

[Process]
Echo,"Processing %ScriptTitle%..."

If,%SourceArch%,Equal,x86,Exit,"Error: Azul Zulu JRE is only available for x64 builds."

If,%cb_RunFromRam%,Equal,True,RunFromRam

///////////////////////////////////////////////////////////////////////////////////
// Extract
If,Not,ExistFile,"%ProgramsCache%\%ProgramFolder%\%SetupFile%",Run,%ScriptFile%,DownloadProgram
Run,%ScriptFile%,ExtractProgram,"%TargetPrograms%\%ProgramFolder%"

///////////////////////////////////////////////////////////////////////////////////
// Settings

Echo,"Configuring %ScriptTitle%..."
RegLoadHives

// Register .jar extension
RegWrite,HKLM,0x1,"Tmp_Software\Classes\.jar","","Zulu.jarfile"
RegWrite,HKLM,0x1,"Tmp_Software\Classes\.jar","Content Type","application/jar"
RegWrite,HKLM,0x1,"Tmp_Software\Classes\Zulu.jarfile","","Zulu jar file"
RegWrite,HKLM,0x1,"Tmp_Software\Classes\Zulu.jarfile\shell\open","","Open"
RegWrite,HKLM,0x1,"Tmp_Software\Classes\Zulu.jarfile\shell\open\command","","#$q%PEPrograms%\%ProgramFolder%\bin\javaw.exe#$q -jar #$q#$p1#$q #$p*"

// x64
RegWrite,HKLM,0x1,"Tmp_Software\Azul Systems\Zulu\zulu-21-jre","CurrentVersion","%ZuluVersionx64%"
RegWrite,HKLM,0x1,"Tmp_Software\Azul Systems\Zulu\zulu-21-jre","InstallationPath","%PEPrograms%\%ProgramFolder%\"
RegWrite,HKLM,0x4,"Tmp_Software\Azul Systems\Zulu\zulu-21-jre","MajorVersion",21
RegWrite,HKLM,0x4,"Tmp_Software\Azul Systems\Zulu\zulu-21-jre","MinorVersion",0

// Add Java to system Path
If,%cb_AddJavaToPath%,Equal,True,Begin
  RegRead,HKLM,"Tmp_System\ControlSet001\Control\Session Manager\Environment","Path",%PathVar%
  StrFormat,Replace,%PathVar%,"%PEPrograms%\%ProgramFolder%;","",%PathVar%
  Set,%PathVar%,"%PEPrograms%\%ProgramFolder%;%PathVar%"
  RegWrite,HKLM,0x2,"Tmp_System\ControlSet001\Control\Session Manager\Environment","Path",%PathVar%
End

// JAVA_HOME EnvVar
If,%cb_AddJavaHomeVar%,Equal,True,RegWrite,HKLM,0x1,"Tmp_System\ControlSet001\Control\Session Manager\Environment","JAVA_HOME","%PEPrograms%\%ProgramFolder%\"

// Create Oracle Keys
If,%cb_CreateOracleJavaSoftKeys%,Equal,True,Begin
  RegWrite,HKLM,0x1,"Tmp_Software\JavaSoft\Java Runtime Environment","CurrentVersion","%JREVersion%"
  RegWrite,HKLM,0x1,"Tmp_Software\JavaSoft\Java Runtime Environment\1.21","JavaHome","%PEPrograms%\%ProgramFolder%\"
  RegWrite,HKLM,0x1,"Tmp_Software\JavaSoft\Java Runtime Environment\1.21","MicroVersion","0"
  RegWrite,HKLM,0x1,"Tmp_Software\JavaSoft\Java Runtime Environment\1.21","RuntimeLib","%PEPrograms%\%ProgramFolder%\bin\server\jvm.dll"
  RegWrite,HKLM,0x1,"Tmp_Software\JavaSoft\Java Runtime Environment\%JREVersion%","JavaHome","%PEPrograms%\%ProgramFolder%\"
  RegWrite,HKLM,0x1,"Tmp_Software\JavaSoft\Java Runtime Environment\%JREVersion%","MicroVersion","0"
  RegWrite,HKLM,0x1,"Tmp_Software\JavaSoft\Java Runtime Environment\%JREVersion%","RuntimeLib","%PEPrograms%\%ProgramFolder%\bin\server\jvm.dll"
End

RegUnloadHives

[#ExtractProgram#]
// ===============================================================================================================================
// Name...........: ExtractProgram
// Description....: Extract program files from script/setup files.
// Syntax.........: Run,%ScriptFile%,ExtractProgram,<DestDir>
// Parameters.....: #1 DestDir - The directory where the files will be extracted.
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: 
// ===============================================================================================================================
[ExtractProgram]
Echo,"Extracting files..."
Set,%ZipFolder%,%ZipFolder64%
Decompress,"%ProgramsCache%\%ProgramFolder%\%SetupFile%",%ProjectTemp%
FileCopy,"%ProjectTemp%\%ZipFolder%\*.*",#1

[#DownloadProgram#]
// ===============================================================================================================================
// Name...........: DownloadProgram
// Description....: Download the program files from the author's website.
// Syntax.........: Run,%ScriptFile%,DownloadProgram
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: 
// ===============================================================================================================================
[DownloadProgram]
Echo,"Downloading %ScriptTitle%..."
Set,%DownloadURL%,%DownloadURLx64%
WebGet,%DownloadURL%,"%ProgramsCache%\%ProgramFolder%\%SetupFile%",NOERR
If,Not,#r,Equal,200,Halt,"Download failed: The code returned was [#r]."

[#ClearDownloadCache#]
// ===============================================================================================================================
// Name...........: ClearDownloadCache
// Description....: Remove all downloaded setup and program files.
// Syntax.........: Run,%ScriptFile%,ClearDownloadCache
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_PurgeCache
// ===============================================================================================================================
[ClearDownloadCache]
DirDeleteEx,"%ProgramsCache%\%ProgramFolder%"

[#SetDefaultOptions#]
// ===============================================================================================================================
// Name...........: SetDefaultOptions
// Description....: Sets script options to default values.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: 
// Related........: btn_SetDefaultOptions
// ===============================================================================================================================
[SetDefaultOptions]
System,Cursor,Wait
WriteInterface,Value,%ScriptFile%,Interface,cb_RunFromRam,False
WriteInterface,Value,%ScriptFile%,Interface,cb_AddJavaToPath,True
WriteInterface,Value,%ScriptFile%,Interface,cb_AddJavaHomeVar,False
WriteInterface,Value,%ScriptFile%,Interface,cb_CreateOracleJavaSoftKeys,True
System,Cursor,Normal

[#ToggleAdvancedOptions#]
// ===============================================================================================================================
// Name...........: ToggleAdvancedOptions
// Description....: Show/Hide advanced options.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_AdvancedOptions
// ===============================================================================================================================
[ToggleAdvancedOptions]
Message,"There are no advanced options available for this script."

[#ShowScriptInfo#]
// ===============================================================================================================================
// Name...........: ShowScriptInfo
// Description....: 
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_ScriptInfo
// ===============================================================================================================================
[ShowScriptInfo]
Message,"Azul Zulu Java 21 Runtime Environment Community Access build.#$x#$xOn first run this script will download the setup file (~43 MB) and cache it for future builds."

