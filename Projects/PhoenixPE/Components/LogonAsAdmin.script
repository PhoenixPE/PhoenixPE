[Main]
Title=Logon as Admin
Description=Allows you to switch users and login as the Administrator account.
Author=Homes32
Level=4
Selected=False
Mandatory=False
Version=1.1.0.0
Date=2023-03-04

[Variables]

[Process]
Echo,"Processing %ScriptTitle%..."

///////////////////////////////////////////////////////////////////////////////////
// Dependency Checks

// Terminal Services utilities such as tsdiscon.exe and tscon.exe are only present in
// Education, Professional, and Enterprise editions of Windows.
If,Not,WimExistFile,%SourceInstallWim%,%SourceInstallWimImage%,"\Windows\System32\tsdiscon.exe",Begin
  Message,"Required Dependency Missing:#$x#$xLogon as Administrator requires tools only present in the Education, Professional, and Enterprise editions of Windows.#$x#$xPlease adjust your Wim index or select an source with the proper edition and try again."
  Halt,"Error: Logon as Administrator requires tools only present in the Education, Professional, and Enterprise editions of Windows."
End

///////////////////////////////////////////////////////////////////////////////////
// Extract
ExtractFile,%ScriptFile%,DefaultReg,UsrClass.dat,"%TargetLocalAppData%\Microsoft\Windows"
ExtractAllFiles,%ScriptFile%,Scripts,%TargetSystem32%
Run,%ScriptFile%,ExtractProgram

///////////////////////////////////////////////////////////////////////////////////
// Registry
RegLoadHives

// Secondary Login Service
RegCopy,HKLM,"Tmp_Install_System\ControlSet001\Services\seclogon",HKLM,"Tmp_System\ControlSet001\Services\seclogon"

// Set the Group Policy Service and the Trusted Installer Service to Manual Start
RegWrite,HKLM,0x4,"Tmp_System\ControlSet001\Services\gpsvc","Start",3
RegWrite,HKLM,0x4,"Tmp_System\ControlSet001\Services\TrustedInstaller","Start",3

// TextInputManagementService is required in 20H2+
If,%SourceVer%,Bigger,10.0.22620.0,RegCopy,HKLM,"Tmp_Install_System\ControlSet001\Services\TextInputManagementService",HKLM,"Tmp_System\ControlSet001\Services\TextInputManagementService"

// Turn off UAC Notifications
RegWrite,HKLM,0x4,"Tmp_Software\Microsoft\Windows\CurrentVersion\Policies\System","EnableLUA",0

// Don't delay before switching sessions
RegWrite,HKLM,0x4,"Tmp_Software\Microsoft\Windows\CurrentVersion\Policies\System","DelayedDesktopSwitchTimeout",0

If,Not,%RegistrySoftwareSource%,Equal,"Install.wim",Begin
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\Windows\CurrentVersion\Authentication",HKLM,"Tmp_Software\Microsoft\Windows\CurrentVersion\Authentication"
  RegCopy,HKLM,"Tmp_Install_Software\Microsoft\Windows NT\CurrentVersion\Winlogon",HKLM,"Tmp_Software\Microsoft\Windows NT\CurrentVersion\Winlogon"
End

// Disable Guest login
RegWrite,HKLM,0x4,"Tmp_Software\Microsoft\Windows NT\CurrentVersion\Winlogon\SpecialAccounts\UserList","Guest",0

// Disable Metro UI
RegWrite,HKLM,0x4,"Tmp_Software\Microsoft\Windows NT\CurrentVersion\Winlogon","EnableSIHostIntegration",0

///////////////////////////////////////////////////////////////////////////////////
// Misc Settings

// Use the wallpaper for the lock screen background
If,Not,ExistFile,"%TargetWindows%\Web\Screen\img100.jpg",Begin
  If,ExistFile,"%TargetWindows%\Web\Wallpaper\Windows\img0.jpg",FileCopy,"%TargetWindows%\Web\Wallpaper\Windows\img0.jpg","%TargetWindows%\Web\Screen\img100.jpg"
End

// Use the wallpaper for the Admin account background
// TODO: Might be nice to add an option for a separate admin background
If,ExistFile,"%TargetWindows%\Web\Wallpaper\Windows\img0.jpg",FileCopy,"%TargetWindows%\Web\Wallpaper\Windows\img0.jpg","%TargetSystem32%\winre.jpg"

// Change Admin Name
If,Not,%txt_AdminAcctName%,Equal,"Administrator",Begin
  If,%txt_AdminAcctName%,Equal,"",WriteInterface,Value,%ScriptFile%,Interface,txt_AdminAcctName,"Administrator"
  ExtractFile,%ScriptFile%,Security,Security.7z,%ProjectTemp%
  Decompress,"%ProjectTemp%\Security.7z",%TargetWindows%
  IniWrite,"%TargetWindows%\Security\Templates\unattend.inf","System Access","NewAdministratorName",#$q%txt_AdminAcctName%#$q
  AddAutoRun,PreShell,HideWait,"Updating Security Database...","secedit.exe","/configure /db #$pWinDir#$p\security\database\unattend.sdb /cfg #$pWinDir#$p\security\templates\unattend.inf /log #$pWinDir#$p\security\logs\unattend.log"
End

// Auto Login
If,%cb_AutoAdminLogin%,Equal,True,Begin
  // Setup Admin autologin
  RegWrite,HKLM,0x4,"Tmp_Software\Microsoft\Windows NT\CurrentVersion\Winlogon","AutoAdminLogon",1
  RegWrite,HKLM,0x1,"Tmp_Software\Microsoft\Windows NT\CurrentVersion\Winlogon","DefaultUserName","Administrator"
  RegWrite,HKLM,0x1,"Tmp_Software\Microsoft\Windows NT\CurrentVersion\Winlogon","DefaultPassword",""
  // Process PhoenixPE.au3 on user login. We need to do this here otherwise autorun entries will be processed under the system account
  RegWrite,HKLM,0x1,"Tmp_Software\Microsoft\Windows NT\CurrentVersion\Winlogon","Userinit","userinit.exe,AutoIt3.exe PhoenixPE.au3"
  // Set a flag for our PhoenixPE config script to see if we want to auto switch to admin (can't use AutoAdminLogon because Winlogon.exe deletes it on boot)
  RegWrite,HKLM,0x4,"Tmp_Software\Microsoft\Windows NT\CurrentVersion\Winlogon","AutoSwitchToAdmin",1
End

RegUnloadHives

// Patch LSM
If,%cb_PatchSessionMgr%,Equal,True,Run,%ScriptFile%,PatchSessionMgr

// Shortcuts
If,%cb_DesktopShc%,Equal,True,AddShortcut,Desktop,,%txt_ShortcutName%,"#$pWinDir#$p\System32\SwitchUser.cmd",,,"%SystemRoot%\system32\imageres.dll",74,Minimized
If,%cb_StartMenuShc%,Equal,True,AddShortcut,StartMenu,%txt_StartMenuFolder%,%txt_ShortcutName%,"#$pWinDir#$p\System32\SwitchUser.cmd",,,"%SystemRoot%\system32\imageres.dll",74,Minimized

[#PatchSessionMgr#]
// ===============================================================================================================================
// Name...........: PatchSessionMgr
// Description....: Patch the Local Session Manager (lsm.dll) to enable switching back to SYSTEM from the Admin session.
// Syntax.........: Run,%ScriptFile%,PatchSessionMgr
// Parameters.....: 
// Return values..: 
// Author.........: Homes32, noel blanc
// Remarks........: Normally attempting to switch to the SYSTEM session (tscon 1) from another session will be blocked and
//                  you will get an error similar to: 
//                      Could not connect sessionID 1 to this session, Error code 5023
//                      Error [5023]:The group or resources is not in the correct state to perform the requested operation.
//                  Patching lsm.dll bypasses this restriction and allows the user to freely switch back and forth between the
//                  SYSTEM and Admin accounts in WinPE. 
// Related........: cb_PatchSessionMgr
// ===============================================================================================================================
[PatchSessionMgr]
If,ExistFile,"%TargetSystem32%\lsm.dll",Begin
  Echo,"Patching Local Session Manager..."
  Binmay,"-u #$q%TargetSystem32%\lsm.dll#$q -s 83F90D770AB8B03000000FA3C872 -r 83F90D770AB8B03000000FA3C8EB"
  If,Not,#r,Equal,0,Halt,"ERROR: Patching [%TargetSystem32%\lsm.dll] failed. The error returned was [#r]."
End

