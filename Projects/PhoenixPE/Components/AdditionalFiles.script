[Main]
Title=Additional Files
Description=Add additional files and folders to your build.
Selected=False
Level=4
Author=Homes32
Version=1.1.1.0
Date=2018-02-23
Mandatory=False

[Variables]

[Process]
Echo,"Copying additional %SourceArch% files..."

If,%SourceArch%,Equal,x64,Begin,
  If,Not,%fb_WimFilesx64%,Equal,"",If,ExistDir,%fb_WimFilesx64%,FileCopy,"%fb_WimFilesx64%\*.*",%TargetDir%
  If,Not,%fb_MediaFilesX64%,Equal,"",If,ExistDir,%fb_MediaFilesX64%,FileCopy,"%fb_MediaFilesX64%\*.*",%OutputDir%
End
Else,Begin  
  If,Not,%fb_WimFilesx86%,Equal,"",If,ExistDir,%fb_WimFilesx86%,FileCopy,"%fb_WimFilesx86%\*.*",%TargetDir%
  If,Not,%fb_MediaFilesX86%,Equal,"",If,ExistDir,%fb_MediaFilesX86%,FileCopy,"%fb_MediaFilesX86%\*.*",%OutputDir%
End

[#WimExploreX86#]
// ===============================================================================================================================
// Name...........: WimExploreX86
// Description....: Open the x86 wim directory in explorer.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........:
// ===============================================================================================================================
[WimExploreX86]
If,%fb_FWimilesx86%,Equal,"",WriteInterface,Value,%ScriptFile%,Interface,fb_Filesx86,"#$pBaseDir#$p\Workbench\Additional\x86"
If,Not,ExistDir,%fb_WimFilesx86%,DirMake,%fb_WimFilesx86%
ShellExecuteEx,Explore,%fb_WimFilesx86%

