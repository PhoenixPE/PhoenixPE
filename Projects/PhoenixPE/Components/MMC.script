[Main]
Title=MMC Snap-Ins
Description=Add shortcuts for various Microsoft Management Console (MMC) Snap-Ins
Selected=False
Level=4
Author=Homes32
Version=1.1.0.0
Date=2020-04-05
Mandatory=False

[Variables]

[Process]

If,%cb_compmgmt_DesktopShc%,Equal,True,AddShortcut,Desktop,,%txt_compmgmt_ShortcutName%,"#$pWinDir#$p\System32\compmgmt.msc"
If,%cb_compmgmt_StartMenuShc%,Equal,True,AddShortcut,StartMenu,%txt_compmgmt_StartMenuFolder%,%txt_compmgmt_ShortcutName%,"#$pWinDir#$p\System32\compmgmt.msc"
If,%cb_compmgmt_PinToTaskbar%,Equal,True,PinShortcut,Taskbar,Auto,"#$pWinDir#$p\System32\compmgmt.msc"
If,%cb_compmgmt_PinToStartMenu%,Equal,True,PinShortcut,StartMenu,Auto,"#$pWinDir#$p\System32\compmgmt.msc"

If,%cb_devmgmt_DesktopShc%,Equal,True,AddShortcut,Desktop,,%txt_devmgmt_ShortcutName%,"#$pWinDir#$p\System32\devmgmt.msc"
If,%cb_devmgmt_StartMenuShc%,Equal,True,AddShortcut,StartMenu,%txt_devmgmt_StartMenuFolder%,%txt_devmgmt_ShortcutName%,"#$pWinDir#$p\System32\devmgmt.msc"
If,%cb_devmgmt_PinToTaskbar%,Equal,True,PinShortcut,Taskbar,Auto,"#$pWinDir#$p\System32\devmgmt.msc"
If,%cb_devmgmt_PinToStartMenu%,Equal,True,PinShortcut,StartMenu,Auto,"#$pWinDir#$p\System32\devmgmt.msc"

If,%cb_diskmgmt_DesktopShc%,Equal,True,AddShortcut,Desktop,,%txt_diskmgmt_ShortcutName%,"#$pWinDir#$p\System32\diskmgmt.msc"
If,%cb_diskmgmt_StartMenuShc%,Equal,True,AddShortcut,StartMenu,%txt_diskmgmt_StartMenuFolder%,%txt_diskmgmt_ShortcutName%,"#$pWinDir#$p\System32\diskmgmt.msc"
If,%cb_diskmgmt_PinToTaskbar%,Equal,True,PinShortcut,Taskbar,Auto,"#$pWinDir#$p\System32\diskmgmt.msc"
If,%cb_diskmgmt_PinToStartMenu%,Equal,True,PinShortcut,StartMenu,Auto,"#$pWinDir#$p\System32\diskmgmt.msc"

If,%cb_services_DesktopShc%,Equal,True,AddShortcut,Desktop,,%txt_services_ShortcutName%,"#$pWinDir#$p\System32\services.msc"
If,%cb_services_StartMenuShc%,Equal,True,AddShortcut,StartMenu,%txt_services_StartMenuFolder%,%txt_services_ShortcutName%,"#$pWinDir#$p\System32\services.msc"
If,%cb_services_PinToTaskbar%,Equal,True,PinShortcut,Taskbar,Auto,"#$pWinDir#$p\System32\services.msc"
If,%cb_services_PinToStartMenu%,Equal,True,PinShortcut,StartMenu,Auto,"#$pWinDir#$p\System32\services.msc"

