[Main]
Title=Hive Editor
Description=Mount and Edit your PE registry hives.
Author=Homes32
Level=10
Selected=None
Mandatory=False
Version=1.1.0.0
Date=2021-11-21

[Process]
Halt,"This script is not designed to be run directly."

[#EditHives#]
// ===============================================================================================================================
// Name...........: EditHives
// Description....: Mount the selected hives, launch regedit.exe and wait for it to terminate. Then unmount the selected hives.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: [UnloadHives]
// ===============================================================================================================================
[EditHives]
Run,%ScriptFile%,LoadHives
Echo,"Regedit is running. Please close Regedit to continue."
ShellExecute,Open,"regedit.exe",""
Run,%ScriptFile%,UnloadHives

[#LoadHives#]
// ===============================================================================================================================
// Name...........: LoadHives
// Description....: Mount the selected hives.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: You must unload the hives manually when you are finished with them.
// Related........: [ForceUnloadHives], [UnloadHives]
// ===============================================================================================================================
[LoadHives]
Echo,"Loading selected registry hives..."

Run,%ScriptFile%,CheckMountPrefix

If,%cb_TargetSoftwareHive%,Equal,True,Begin
  If,Not,ExistFile,%RegSoftware%,Halt,"The Software registry hive was not found. Please build the project before attempting to edit it's registry hives!"
  RegHiveLoad,%txt_TargetPrefix%Software,%RegSoftware%
End
If,%cb_TargetSystemHive%,Equal,True,Begin
  If,Not,ExistFile,%RegSystem%,Halt,"The System registry hive was not found. Please build the project before attempting to edit it's registry hives!"
  RegHiveLoad,%txt_TargetPrefix%System,%RegSystem%
End
If,%cb_TargetDefaultHive%,Equal,True,Begin
  If,Not,ExistFile,%RegDefault%,Halt,"The Default registry hive was not found. Please build the project before attempting to edit it's registry hives!"
  RegHiveLoad,%txt_TargetPrefix%Default,%RegDefault%
End  
If,%cb_TargetDriversHive%,Equal,True,Begin
  If,Not,ExistFile,%RegDrivers%,Halt,"The Drivers registry hive was not found. Please build the project before attempting to edit it's registry hives!"
  RegHiveLoad,%txt_TargetPrefix%Drivers,%RegDrivers%
End

If,%cb_InstallSoftwareHive%,Equal,True,RegHiveLoad,%txt_InstallPrefix%Software,%RegInstallSoftware%
If,%cb_InstallSystemHive%,Equal,True,RegHiveLoad,%txt_InstallPrefix%System,%RegInstallSystem%
If,%cb_InstallDefaultHive%,Equal,True,RegHiveLoad,%txt_InstallPrefix%Default,%RegInstallDefault%
If,%cb_InstallDriversHive%,Equal,True,RegHiveLoad,%txt_InstallPrefix%Drivers,%RegInstallDrivers%

If,%cb_CustomSoftwareHive%,Equal,True,RegHiveLoad,%txt_CustomPrefix%Software,"%fb_CustomHives%\SOFTWARE"
If,%cb_CustomSystemHive%,Equal,True,RegHiveLoad,%txt_CustomPrefix%System,"%fb_CustomHives%\SYSTEM"
If,%cb_CustomDefaultHive%,Equal,True,RegHiveLoad,%txt_CustomPrefix%Default,"%fb_CustomHives%\DEFAULT"
If,%cb_CustomDriversHive%,Equal,True,RegHiveLoad,%txt_CustomPrefix%Drivers,"%fb_CustomHives%\DRIVERS"

[#UnloadHives#]
// ===============================================================================================================================
// Name...........: UnloadHives
// Description....: Unload the selected hives.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: [LoadHives]
// ===============================================================================================================================
[UnloadHives]
Echo,"Unloading registry hives..."

Run,%ScriptFile%,CheckMountPrefix

If,%cb_TargetSoftwareHive%,Equal,True,RegHiveUnLoad,%txt_TargetPrefix%Software
If,%cb_TargetSystemHive%,Equal,True,RegHiveUnLoad,%txt_TargetPrefix%System
If,%cb_TargetDefaultHive%,Equal,True,RegHiveUnLoad,%txt_TargetPrefix%Default
If,%cb_TargetDriversHive%,Equal,True,RegHiveUnLoad,%txt_TargetPrefix%Drivers

If,%cb_InstallSoftwareHive%,Equal,True,RegHiveUnLoad,%txt_InstallPrefix%Software
If,%cb_InstallSystemHive%,Equal,True,RegHiveUnLoad,%txt_InstallPrefix%System
If,%cb_InstallDefaultHive%,Equal,True,RegHiveUnLoad,%txt_InstallPrefix%Default
If,%cb_InstallDriversHive%,Equal,True,RegHiveUnLoad,%txt_InstallPrefix%Drivers

If,%cb_CustomSoftwareHive%,Equal,True,RegHiveUnLoad,%txt_CustomPrefix%Software
If,%cb_CustomSystemHive%,Equal,True,RegHiveUnLoad,%txt_CustomPrefix%System
If,%cb_CustomDefaultHive%,Equal,True,RegHiveUnLoad,%txt_CustomPrefix%Default
If,%cb_CustomDriversHive%,Equal,True,RegHiveUnLoad,%txt_CustomPrefix%Drivers

Run,%ScriptFile%,Cleanup-TransactionLogs

[ForceUnloadHives]
// ===============================================================================================================================
// Name...........: ForceUnloadHives
// Description....: Unload ALL registry hives mounted to HKLM regardless of whether or not we mounted them.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: [ForceUnloadHives], [UnloadHives]
// ===============================================================================================================================
[ForceUnloadHives]
Echo,"Unloading all registry hives..."
RegUnloadHives,Force
Run,%ScriptFile%,Cleanup-TransactionLogs

// ===============================================================================================================================
// Name...........: CheckMountPrefix
// Description....: Verify Hive Prefix values from the user interface are populated.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: [ForceUnloadHives], [UnloadHives]
// ===============================================================================================================================
[CheckMountPrefix]
If,%txt_TargetPrefix%,Equal,"",Begin
  Message,"You must specify a prefix for the Target Hive Mount-point.",ERROR
  Halt,"You must specify a prefix for the Target Hive Mount-point."
End
If,%txt_InstallPrefix%,Equal,"",Begin
  Message,"You must specify a prefix for the Install Hive Mount-point.",ERROR
  Halt,"You must specify a prefix for the Install Hive Mount-point."
End
If,%txt_CustomPrefix%,Equal,"",Begin
  Message,"You must specify a prefix for the Custom Hive Mount-point.",ERROR
  Halt,"You must specify a prefix for the Custom Hive Mount-point."
End  

[#Cleanup-TransactionLogs#]
// ===============================================================================================================================
// Name...........: Cleanup-TransactionLogs
// Description....: Remove transaction logs created by our reg modifications.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: [ForceUnloadHives], [UnloadHives]
// ===============================================================================================================================
[Cleanup-TransactionLogs]
Echo,"Cleaning up transaction logs..."

// Reg Cache
FileDeleteEx,%RegistryCache%\*.LOG1
FileDeleteEx,%RegistryCache%\*.LOG2
FileDeleteEx,%RegistryCache%\*.blf
FileDeleteEx,%RegistryCache%\*.regtrans-ms

// Target
FileDeleteEx,%TargetDir%\Windows\System32\config\*.LOG1
FileDeleteEx,%TargetDir%\Windows\System32\config\*.LOG2
FileDeleteEx,%TargetDir%\Windows\System32\config\*.blf
FileDeleteEx,%TargetDir%\Windows\System32\config\*.regtrans-ms

// Custom
FileDeleteEx,%fb_CustomHives%\Windows\System32\config\*.LOG1
FileDeleteEx,%fb_CustomHives%\Windows\System32\config\*.LOG2
FileDeleteEx,%fb_CustomHives%\Windows\System32\config\*.blf
FileDeleteEx,%fb_CustomHives%\Windows\System32\config\*.regtrans-ms

[#SetDefaultOptions#]
// ===============================================================================================================================
// Name...........: SetDefaultOptions
// Description....: Sets script options to default values.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: 
// Related........: btn_SetDefaultOptions
// ===============================================================================================================================
[SetDefaultOptions]
System,Cursor,Wait
WriteInterface,Value,%ScriptFile%,Interface,cb_TargetSoftwareHive,True
WriteInterface,Value,%ScriptFile%,Interface,cb_TargetSystemHive,True
WriteInterface,Value,%ScriptFile%,Interface,cb_TargetDefaultHive,True
WriteInterface,Value,%ScriptFile%,Interface,cb_TargetDriversHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_InstallSoftwareHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_InstallSystemHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_InstallDefaultHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_InstallDriversHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_CustomSoftwareHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_CustomSystemHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_CustomDefaultHive,False
WriteInterface,Value,%ScriptFile%,Interface,cb_CustomDriversHive,False
WriteInterface,Value,%ScriptFile%,Interface,fb_CustomHives,""
WriteInterface,Value,%ScriptFile%,Interface,txt_TargetPrefix,"Tmp_"
WriteInterface,Value,%ScriptFile%,Interface,txt_InstallPrefix,"Tmp_Install_"
WriteInterface,Value,%ScriptFile%,Interface,txt_CustomPrefix,"Tmp_Custom_"
System,Cursor,Normal

[#ToggleAdvancedOptions#]
// ===============================================================================================================================
// Name...........: ToggleAdvancedOptions
// Description....: Show/Hide Advanced options in the script's GUI
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: 
// Related........: Called by [Interface] btn_AdvancedOptions
// ===============================================================================================================================
[ToggleAdvancedOptions]
System,SetLocal
ReadInterface,Visible,%ScriptFile%,Interface,bvl_CustomHives,%state%
If,%state%,Equal,False,Begin
  If,Question,"WARNING!#$x#$xAdvanced options are intended for expert users only!#$x#$xModifying advanced options without knowing what you are doing can cause permanent damage to your project and may result in unexpected behavior and/or build failures.#$x#$xAre you sure you want to continue?",Begin
	Set,%Toggle%,True
    WriteInterface,Resource,%ScriptFile%,Interface,btn_AdvancedOptions,Advanced_Exit_16.png
	WriteInterface,Tooltip,%ScriptFile%,Interface,btn_AdvancedOptions,"Hide Advanced Options"
  End
  Else,Set,%Toggle%,False
End
Else,Begin
  Set,%Toggle%,False
  WriteInterface,Resource,%ScriptFile%,Interface,btn_AdvancedOptions,Advanced_16.png
  WriteInterface,Tooltip,%ScriptFile%,Interface,btn_AdvancedOptions,"Show Advanced Options"
End

WriteInterface,Visible,%ScriptFile%,Interface,btn_LoadHives,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,bvl_InstallHives,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_InstallSoftwareHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_InstallSystemHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_InstallDefaultHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_InstallDriversHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,bvl_CustomHives,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_CustomSoftwareHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_CustomSystemHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_CustomDefaultHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,cb_CustomDriversHive,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,lbl_CustomHivePath,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,fb_CustomHives,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,lbl_CustomHiveInfo,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,bvl_HiveMountPrefix,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,txt_TargetPrefix,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,txt_InstallPrefix,%Toggle%
WriteInterface,Visible,%ScriptFile%,Interface,txt_CustomPrefix,%Toggle%
System,EndLocal

[#ShowScriptInfo#]
// ===============================================================================================================================
// Name...........: ShowScriptInfo
// Description....: 
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_ScriptInfo
